{
  "name": "putainde-localstorage",
  "version": "2.1.0",
  "description": "A simple localStorage abstraction",
  "keywords": [
    "localStorage",
    "storage"
  ],
  "author": "Maxime Thirouin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/putaindecode/localstorage.git"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "index.js"
  ],
  "devDependencies": {
    "jscs": "^1.5.9",
    "jshint": "^2.5.2",
    "jshint-stylish": "^0.4.0",
    "tap-colorize": "^1.2.0",
    "tape": "^2.13.4",
    "testling": "^1.7.0"
  },
  "scripts": {
    "jscs": "jscs *.js **/*.js",
    "jshint": "jshint . --exclude node_modules --reporter node_modules/jshint-stylish/stylish.js",
    "test": "npm run jscs && npm run jshint && testling | tap-colorize"
  },
  "testling": {
    "files": "test/**/*.js",
    "browsers": [
      "ie/8..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/6..latest",
      "opera/11.0..latest",
      "iphone/6..latest",
      "ipad/6..latest",
      "android-browser/latest"
    ]
  }
}
